{
  "extends": ["airbnb", "prettier", "react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [2, 2],
    "space-before-function-paren": "off"
    // "no-tabs": 0,
    // "react/prop-types": 0,
    // "react/jsx-indent": [2, "tab"],
    // "react/jsx-indent-props": [2, "tab"]
  }
}
